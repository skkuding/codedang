{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "k6: Run Submission Test (dotenv)",
      "type": "shell",
      "command": "npx",
      "args": [
        "dotenv-cli",
        "-e",
        "${workspaceFolder}/loadtest/.env",
        "-e",
        "${workspaceFolder}/loadtest/.env.development",
        "--",
        "k6",
        "run",
        "--out",
        "experimental-opentelemetry",
        "--out",
        "json=${workspaceFolder}/loadtest/submission/results/submission-test-result.json",
        "${workspaceFolder}/loadtest/submission/index.ts"
      ],
      "group": {
        "kind": "test",
        "isDefault": true
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared",
        "showReuseMessage": true,
        "clear": false
      },
      "problemMatcher": []
    }
  ]
}
