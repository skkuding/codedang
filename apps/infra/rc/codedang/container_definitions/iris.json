{
  "name": "Codedang-Iris",
  "image": "${ecr_uri}",
  "essential": true,
  "privileged": true,
  "linuxParameters": {
    "cgroupnsMode": "host"
  },
  "environment": [
    {
      "name": "APP_ENV",
      "value": "production"
    },
    {
      "name": "DATABASE_URL",
      "value": "${database_url}"
    },
    {
      "name": "RABBITMQ_SSL",
      "value": "true"
    },
    {
      "name": "RABBITMQ_HOST",
      "value": "${rabbitmq_host}"
    },
    {
      "name": "RABBITMQ_PORT",
      "value": "${rabbitmq_port}"
    },
    {
      "name": "RABBITMQ_DEFAULT_USER",
      "value": "${rabbitmq_username}"
    },
    {
      "name": "RABBITMQ_DEFAULT_PASS",
      "value": "${rabbitmq_password}"
    },
    {
      "name": "RABBITMQ_DEFAULT_VHOST",
      "value": "${rabbitmq_vhost}"
    },
    {
      "name": "OTEL_EXPORTER_OTLP_ENDPOINT_URL",
      "value": "${otel_exporter_otlp_endpoint_url}"
    }
  ],
  "logConfiguration": {
    "logDriver": "awsfirelens",
    "secretOptions": []
  }
}
