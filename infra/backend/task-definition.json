[
  {
    "name": "Codedang-Api",
    "image": "219857217698.dkr.ecr.ap-northeast-1.amazonaws.com/codedang-api",
    "cpu": 256,
    "memory": 512,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 3000
      }
    ],
    "environment": [
      {
        "name": "DATABASE_URL",
        "value": "${local.postgres_url}"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "ecs/Codedang-Api",
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }
]
