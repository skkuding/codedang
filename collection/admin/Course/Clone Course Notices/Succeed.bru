meta {
  name: Succeed
  type: graphql
  seq: 1
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  mutation ($courseNoticeIds: [Int!]!, $cloneToId: Int!) {
    cloneCourseNotices(courseNoticeIds: $courseNoticeIds, cloneToId: $cloneToId) {
      id
      createdById
      title
      content
      isPublic
      isFixed
      createTime
      updateTime
      groupId
    }
  }
}

body:graphql:vars {
  {
    "courseNoticeIds": [
      1,2,3
    ],
    "cloneToId": 2
  }
}

assert {
  res.body.data.cloneCourseNotices: isDefined
}

