meta {
  name: Succeed
  type: graphql
  seq: 1
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  mutation($courseNoticeId: Int! ) {
    deleteCourseNotice(courseNoticeId: $courseNoticeId) {
      title
      content
      isPublic
      isFixed
      createTime
      updateTime
      id
      createdById
      groupId
    }
  }
}

body:graphql:vars {
  {
    "courseNoticeId": 1
  }
}

assert {
  res.body.data.deleteCourseNotice: isDefined
}

