meta {
  name: Get Check Request
  type: graphql
  seq: 1
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: inherit
}

body:graphql {
  query GetCheckRequests(
    $assignmentId: Int!,
    $problemId: Int!,
    $groupId: Int!
  ) {
    getCheckRequests(
      assignmentId: $assignmentId,
      problemId: $problemId,
      groupId: $groupId
    ) {
      id
      language
      enableMerging
      minTokens
      useJplagClustering
      createTime
      result
    }
  }
}

body:graphql:vars {
  {
    "problemId": 4,
    "assignmentId": 6,
    "groupId": 1
  }
}

