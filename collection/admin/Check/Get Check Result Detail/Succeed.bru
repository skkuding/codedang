meta {
  name: Get Result Detail
  type: graphql
  seq: 1
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: inherit
}

body:graphql {
  query GetCheckResultDetails(
    $resultId: Int!,
    $groupId: Int!
  ) {
    getCheckResultDetails(
      resultId: $resultId,
      groupId: $groupId
    ) {
      requestId
      firstCheckSubmissionId
      secondCheckSubmissionId
      averageSimilarity
      maxSimilarity
      maxLength
      longestMatch
      matches {
        startInFirst {
          line
          column
        }
        endInFirst {
          line
          column
        }
        startInSecond {
          line
          column
        }
        endInSecond {
          line
          column
        }
        lengthOfFirst
        lengthOfSecond
      }
      firstSimilarity
      secondSimilarity
      clusterId
    }
  }
}

body:graphql:vars {
  {
    "resultId": 1,
    "groupId": 1
  }
}

