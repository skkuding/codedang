meta {
  name: Succeed
  type: graphql
  seq: 1
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  query GetContest($contestId: Int!) {
    getContest(contestId: $contestId) {
      id
      title
      participants
    }
  }
}

body:graphql:vars {
  {
    "contestId": 1
  }
}

assert {
  res.body.data.getContest: isDefined
}

docs {
