meta {
  name: [ERR] Nonexistent Problem
  type: graphql
  seq: 2
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  query GetContestsByProblemId($problemId: Int!) {
    getContestsByProblemId(problemId: $problemId) {
      upcoming {
        id
      }
      ongoing {
        id
      }
      finished {
        id
      }
    }
  }
}

body:graphql:vars {
  {
    "problemId": 999
  }
}

