meta {
  name: Succeed
  type: graphql
  seq: 1
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  mutation DeleteContestQnA($contestId: Int!, $order: Int!) {
    deleteContestQnA(contestId: $contestId, order: $order) {
      id
      title
      content
      createTime
      createdById
    }
  }
}

body:graphql:vars {
  {
    "contestId": 20,
    "order": 1
  }
}

assert {
  res.body.data.deleteContestQnA: isDefined
}

script:pre-request {
  await require("./login").loginContestAdmin(req);
}

