meta {
  name: [ERR] Nonexistent UserContest
  type: graphql
  seq: 2
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: inherit
}

body:graphql {
  mutation ToggleParticipantBlocking(
    $userId: Int!,
    $contestId: Int!
  ) {
    toggleParticipantBlocking(
      userId: $userId
      contestId: $contestId
    ) {
      id
      role
      userId
      contestId
      isBlocked
    }
  }
}

body:graphql:vars {
  {
    "userId" : 999,
    "contestId" : 19
  }
}

script:pre-request {
  await require("./login").loginContestAdmin(req);
}

settings {
  encodeUrl: true
  timeout: 0
}

