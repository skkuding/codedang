post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  query GetContestUpdateHistories($contestId: Int!) {
    getContestUpdateHistories(contestId: $contestId) {
      updateHistories {
        id
        order
        problemId
        updatedAt
        updatedFields
        updatedInfo
      }
    }
  }
}

body:graphql:vars {
  {
    "contestId": 1
  }
}

