meta {
  name: [ERR] Required Fields Not Entered
  type: graphql
  seq: 3
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  mutation CreateContest($input: CreateContestInput!) {
    createContest(input: $input) {
      id
    }
  }
}

body:graphql:vars {
  {
    "input": {
      "description": "new contest without title",
      "startTime": "2026-01-01T12:00:00.000Z",
      "endTime": "2030-01-01T12:00:00.000Z",
      "registerDueTime": "2026-01-02T12:00:00.000Z",
      "isJudgeResultVisible": true,
      "userContest": []
    }
  }
}

script:pre-request {
  await require("./login").loginContestAdmin(req);
}

assert {
  res.body.errors[0].message: eq Title was not provided
}

docs {
  # 📙 [ERR] Required Fields Not Entered

  **POST** `/graphql (mutation: createContest)`

  Title String was not provided.

  필수 input(`title`)을 입력하지 않았을 때,

  `BAD_USER_INPUT`이 발생합니다.
}
