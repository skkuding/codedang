  auth: none
}

body:graphql {
  mutation ToggleContestQnAResolved(
    $contestId: Int!,
    $qnAOrder: Int!
  ) {
    toggleContestQnAResolved(
      contestId: $contestId,
      qnAOrder: $qnAOrder
    ) {
      id
    }
  }
}

body:graphql:vars {
  {
    "contestId": 1,
    "qnAOrder": 2
  }
}

script:pre-request {
  await require("./login").loginAdmin(req);
}


