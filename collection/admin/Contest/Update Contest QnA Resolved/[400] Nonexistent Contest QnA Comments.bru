meta {
  name: [400] Nonexistent Contest QnA Comments
  type: graphql
  seq: 2
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  mutation {
    toggleContestQnAResolved(
      contestId: 1
      qnAOrder: 2
    ) {
      id
      order
      isResolved
    }
  }
}

script:pre-request {
  await require("./login").loginAdmin(req);
}
