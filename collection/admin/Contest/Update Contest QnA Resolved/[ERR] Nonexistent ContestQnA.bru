meta {
  name: [ERR] Nonexistent ContestQnA
  type: graphql
  seq: 2
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  mutation ToggleContestQnAResolved(
    $contestId: Int!,
    $qnAOrder: Int!
  ) {
    toggleContestQnAResolved(
      contestId: $contestId,
      qnAOrder: $qnAOrder
    ) {
      id
    }
  }
}

body:graphql:vars {
  {
    "contestId": 20,
    "qnAOrder": 999
  }
}

script:pre-request {
  await require("./login").loginContestAdmin(req);
}

