meta {
  name: Succeed
  type: graphql
  seq: 1
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  mutation DeleteContestQnAComment(
    $contestId: Int!
    $qnAOrder: Int!
    $commentOrder: Int!
  ) {
    deleteContestQnAComment(
      contestId: $contestId
      qnAOrder: $qnAOrder
      commentOrder: $commentOrder
    ) {
      id
      content
      isContestStaff
      order
      contestQnAId
      createdTime
    }
  }
}

body:graphql:vars {
  {
    "contestId": 20,
    "qnAOrder": 1,
    "commentOrder": 1
  }
}

script:pre-request {
  await require("./login").loginContestAdmin(req);
}

