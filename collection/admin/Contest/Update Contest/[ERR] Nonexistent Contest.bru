meta {
  name: [ERR] Nonexistent Contest
  type: graphql
  seq: 2
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  mutation UpdateContest(
    $contestId: Int!,
    $input: UpdateContestInput!
  ) {
    updateContest(
      contestId: $contestId
      input: $input
    ) {
      id
    }
  }
}

body:graphql:vars {
  {
    "contestId": 999,
    "input": {
      "title": "Nonexistent Contest"
    }
  }
}

script:pre-request {
  await require("./login").loginContestAdmin(req);
}

