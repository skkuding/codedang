meta {
  name: [ERR] Nonexistent Contest
  type: graphql
  seq: 2
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  query GetContestScoreSummaries(
    $contestId: Int!
    $take: Int
    $cursor: Int
    $searchingName: String
  ) {
    getContestScoreSummaries(
      contestId: $contestId
      take: $take
      cursor: $cursor
      searchingName: $searchingName
    ) {
      userId
    }
  }
}

body:graphql:vars {
  {
    "contestId": 999
  }
}

script:pre-request {
  await require("./login").loginContestAdmin(req);
}

