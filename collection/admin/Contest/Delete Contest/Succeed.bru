meta {
  name: Succeed
  type: graphql
  seq: 1
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  mutation DeleteContest($contestId: Int!) {
    deleteContest(contestId: $contestId) {
      id
      title
      description
      startTime
      endTime
    }
  }
}

body:graphql:vars {
  {
    "contestId": 20
  }
}

assert {
  res.body.data.deleteContest: isDefined
}

script:pre-request {
  await require("./login").loginContestAdmin(req);
}

