meta {
  name: Success
  type: graphql
  seq: 1
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  query getContestSubmissionSummaries($contestId: Int!, $userId: Int!) {
    getContestSubmissionSummaries(contestId: $contestId, userId: $userId) {
      contestId
      problemTitle
      language
      submissionTime
      ip
      codeSize
      submissionResult
    }
  }
}

body:graphql:vars {
  {
    "contestId": 1,
    "userId": 4
  }
}
