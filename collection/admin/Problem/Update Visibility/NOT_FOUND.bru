meta {
  name: NOT_FOUND
  type: graphql
  seq: 2
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  mutation UpdateVisibility($problemId: Int!, $isVisible: Boolean!) {
    updateVisibility(problemId: $problemId, isVisible: $isVisible) {
      id
      title
      isVisible
      updateTime
    }
  }
}

body:graphql:vars {
  {
    "problemId": 99999,
    "isVisible": false
  }
}

assert {
  res.body.errors[0].extensions.code: eq NOT_FOUND
}
