meta {
  name: Succeed
  type: graphql
  seq: 1
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  query GetAnnouncementsByContestId($contestId: Int!) {
    announcementByContestId(contestId: $contestId) {
      id
      contestId
      problemId
  		problemOrder
      content
      createTime
      updateTime
    }
  }
}

body:graphql:vars {
  {
    "contestId": 20
  }
}
