body:graphql {
  query GetAnnouncementById($contestId: Int!, $id: Int!) {
    announcement(
      contestId: $contestId
      id: $id
    ) {
      id
      content
      createTime
      updateTime
    }
  }
}

body:graphql:vars {
  {
    "contestId": 20,
    "id": 1
  }
}

