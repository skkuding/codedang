meta {
  name: [ERR] Nonexistent Announcement
  type: graphql
  seq: 2
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  query GetAnnouncementById($contestId: Int!, $id: Int!) {
    announcement(
      contestId: $contestId
      id: $id
    ) {
      id
      content
      createTime
      updateTime
    }
  }
}

body:graphql:vars {
  {
    "contestId" : 20,
    "id": 999
  }
}
