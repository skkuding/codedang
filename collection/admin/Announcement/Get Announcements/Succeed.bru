post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  query {
    getAnnouncements(
      contestId: 1
      problemId: 1
    ) {
    	id
      problemId
      contestId
      content
    }
  }
  
  
}

docs {
