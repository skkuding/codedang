meta {
  name: Error: Invalid Announcement ID
  type: graphql
  seq: 2
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  mutation($id: Int!) {
    removeAnnouncement(
      id: $id
    ) {
      id
      problemId
      content
    }
  }
}

body:graphql:vars {
  {
    "id": -999999
  }
}

assert {
  res.body.errors: isDefined
}
