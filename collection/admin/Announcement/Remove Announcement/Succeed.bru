meta {
  name: Succeed
  type: graphql
  seq: 1
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  mutation {
    removeAnnouncement(
      contestId: 20
      id: 1
    ) {
      id
      content
      createTime
      updateTime
    }
  }
}

docs {
