body:graphql {
  query GetSubmissionCountByLanguage(
    $assignmentId: Int!,
    $problemId: Int!,
    $groupId: Int!
  ) {
    getLatestSubmissionCountByLanguage(
      assignmentId: $assignmentId,
      problemId: $problemId,
      groupId: $groupId
    ) {
  		language
      count
    }
  }
}

