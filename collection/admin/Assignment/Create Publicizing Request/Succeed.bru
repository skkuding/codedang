body:graphql {
  mutation CreatePublicizingRequest($assignmentId: Int!, $groupId: Int!){
    createPublicizingRequest(
      assignmentId: $assignmentId,
      groupId: $groupId
    ) {
      assignmentId
      expireTime
      userId
    }
  }
}

assert {
  res.body.data.createPublicizingRequest: isDefined
}
