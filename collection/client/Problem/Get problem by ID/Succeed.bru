meta {
  name: Succeed
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/problem/1
  body: none
  auth: none
}

query {
  ~groupId: 2
}

assert {
  res.status: eq 200
  res("id"): isNumber 1
  res("title"): isString
  res("description"): isString
  res("inputDescription"): isString
  res("outputDescription"): isString
  res("hint"): isString
  res("languages"): isDefined
  res("timeLimit"): isNumber
  res("memoryLimit"): isNumber
  res("difficulty"): isString
  res("source"): isString
}
