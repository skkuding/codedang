meta {
  name: Succeed(distribution mode)
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/contest/:contestId/problem/:problemId/statistics/graph?mode=distribution
  body: none
  auth: inherit
}

params:query {
  mode: distribution
}

params:path {
  contestId: 6
  problemId: 5
}

assert {
  res.status: eq 200
  res("contestId"): isNumber
  res("problemId"): isNumber
  res("mode"): eq "distribution"
  res("distribution"): isDefined
  res("distribution.totalSubmissions"): isNumber
  res("distribution.counts"): isDefined
}

settings {
  encodeUrl: true
  timeout: 0
}

