meta {
  name: Succeed
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/contest/:contestId/problem/:problemId/statistics/graph
  body: none
  auth: inherit
}

params:path {
  contestId: 6
  problemId: 5
}

assert {
  res.status: eq 200
  res("contestId"): isNumber
  res("problemId"): isNumber
  res("mode"): isString
  res("distribution"): isDefined
  res("timeline"): isDefined
}

settings {
  encodeUrl: true
  timeout: 0
}

assert {
  res.status: eq 403
  res("message"): eq Contest problem statistics are available after the contest ends
}

