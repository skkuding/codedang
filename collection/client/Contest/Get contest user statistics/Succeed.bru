meta {
  name: Succeed
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/contest/:contestId/statistics/user/:userId
  body: none
  auth: inherit
}

params:path {
  contestId: 6
  userId: 10
}

assert {
  res.status: eq 200
  res("user"): isDefined
  res("user.rank"): isNumber
  res("user.username"): isString
  res("user.totalSolved"): isNumber
  res("user.totalPenalty"): isNumber
  res("user.problemAnalysis"): isArray
  res("user.timeline"): isArray
  res("user.submissionHistory"): isArray
}

settings {
  encodeUrl: true
  timeout: 0
}

