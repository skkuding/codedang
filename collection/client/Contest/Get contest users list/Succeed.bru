meta {
  name: Succeed
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/contest/:contestId/statistics/users
  body: none
  auth: inherit
}

params:path {
  contestId: 6
}

assert {
  res.status: eq 200
  res: isArray
}

settings {
  encodeUrl: true
  timeout: 0
}

assert {
  res.status: eq 403
  res("message"): eq Contest has not ended
}

