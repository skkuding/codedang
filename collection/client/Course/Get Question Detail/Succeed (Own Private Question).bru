tests {
  test("should allow user to view their own private question", function() {
    const data = res.getBody();
    expect(res.getStatus()).to.equal(200);
    expect(data).to.have.property('id');
    expect(data.isPrivate).to.equal(true);
    // User should be able to see their own private question
  });
}

