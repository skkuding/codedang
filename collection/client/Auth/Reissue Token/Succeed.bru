get {
  url: {{baseUrl}}/auth/reissue
  body: none
  auth: none
}

vars:post-response {
  jwtToken: res.headers.authorization
}

assert {
  res.status: 200
  res.headers.authorization: contains Bearer
}

script:pre-request {
  await require("./login").loginUser(req);
}

