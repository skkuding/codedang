meta {
  name: Succeed
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/announcement?contestId=1
  body: none
  auth: none
}

params:query {
  contestId: 1
}

assert {
  res.status: eq 200
  res.body[0].id: isNumber
  res.body[0].content: isString
  res.body[0].createTime: isString
  res.body[0].updateTime: isString
  ~res.body[0].problemOrder: isNumber
}

docs {
  # 📘 Get Announcements
  
