meta {
  name: Succeed
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/course_notice/unreadCount
  body: none
  auth: inherit
}

settings {
  encodeUrl: true
}

assert {
  res.status: eq 200
  res.body.data[0].unreadCount: isNumber
}

script:pre-request {
  await require("./login").loginUser(req);
}

