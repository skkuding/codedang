get {
  url: {{baseUrl}}/group/joined
  body: none
  auth: none
}

assert {
  res.status: eq 200
  res.body[0].id: isNumber
  res.body[0].groupName: isString
  res.body[0].description: isString
  res.body[0].memberNum: isNumber
  res.body[0].isGroupLeader: isBoolean
}

