script:pre-request {
  const axios = require("axios");
  
  await require("./login").loginUser(req);
  
  const baseUrl = bru.getEnvVar("baseUrl");
  const authorization = bru.getVar("jwtToken");
  try {
    await axios.post(
      baseUrl + "/group/4/join",
      {},
      { headers: { authorization } }
    )
  } catch (error) {}
}
