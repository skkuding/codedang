meta {
  name: Succeed
  type: graphql
  seq: 1
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  query GetAssignment($assignmentId: Int!, $groupId: Int!) {
    getAssignment(assignmentId: $assignmentId, groupId: $groupId) {
      id
      title
      week
      participants
    }
  }
  
}

body:graphql:vars {
  {
    "assignmentId": 5,
    "groupId": 2
  }
}

assert {
  res.body.data.getAssignment: isDefined
}

