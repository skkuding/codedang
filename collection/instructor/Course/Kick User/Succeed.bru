meta {
  name: Succeed
  type: graphql
  seq: 1
}

post {
  url: {{gqlUrl}}
  body: graphql
  auth: none
}

body:graphql {
  mutation {
    kickUser(groupId: 2, userId: 5){
      userId
      groupId
      isGroupLeader
    }
  }
  
}

assert {
  res.status: eq 200
  res.body.data.issueInvitation: isString
}

docs {
  # Issue Invitation
