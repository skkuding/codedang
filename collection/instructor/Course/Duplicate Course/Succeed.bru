body:graphql {
  mutation {
    duplicateCourse(groupId: 2) {
      id
      groupName
      groupType
      config
      courseInfo {
        courseNum
        classNum
        professor
        semester
        week
        email
        website
        office
        phoneNum
      }
    }
  }

}

